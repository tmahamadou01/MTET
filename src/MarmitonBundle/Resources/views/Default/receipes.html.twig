<section class="padT100 padB20">
<div class="filter-section marB70">
    <div class="container">
        <div class="row ">
            <form class="marB20">
                <div class="col-md-3 col-sm-3 col-xs-12 filter-box marB30">
                    <label>Select categorie</label>
                    <select>
                        <option value="lunch">Lunch</option>
                        <option value="breakfast">Breakfast</option>
                        <option value="desert">Desert</option>
                        <option value="pizza">Fresh pizza</option>
                        <option value="dinner">Dinner</option>
                    </select>
                </div>
                <div class="col-md-3 col-sm-3 col-xs-12 filter-box marB30">
                    <label>Select type</label>
                    <select>
                        <option value="veg">Veg</option>
                        <option value="non-veg">Non veg</option>
                    </select>
                </div>
                <div class="col-md-3 col-sm-3 col-xs-12 filter-box marB30">
                    <label>Select drink</label>
                    <select>
                        <option value="All">Cold drink</option>
                        <option value="Men">Juice</option>
                        <option value="Women">Bear</option>
                        <option value="Kids">Soda</option>
                    </select>
                </div>
                <div class="col-md-3 col-sm-3 col-xs-12 filter-box marB30">
                    <label>Select menu</label>
                    <select>
                        <option value="All">Muffins</option>
                        <option value="Men">BBQ chicken</option>
                        <option value="Women">Salade</option>
                        <option value="Kids">Chinese food</option>
                    </select>
                </div>
            </form>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-md-4 col-sm-4 col-xs-12 marB30">
                <h4 class="marB20">Filter dishe by price</h4>
                <div id="slider-range-one" class="filter-slider"></div>
                <p><input type="text" id="amount1" readonly>
                </p>
                <p>Filter by : <span>min price</span> to <span>max price</span></p>
            </div>
            <div class="col-md-4 col-sm-4 col-xs-12 marB30">
                <h4 class="marB20">Filter dishe by weight</h4>
                <div id="slider-range-two" class="filter-slider"></div>
                <p><input type="text" id="amount2" readonly>
                </p>
                <p>Filter by : <span>min price</span> to <span>max price</span></p>
            </div>
            <div class="col-md-4 col-sm-4 col-xs-12 marB30">
                <h4 class="marB20">Popular dish since</h4>
                <div id="slider-range-three" class="filter-slider"></div>
                <p><input type="text" id="amount3" readonly>
                </p>
                <p>Filter by : <span>min price</span> to <span>max price</span></p>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <h4 class="marB20">Related product by :</h4>
                <div class="filter-check-box marB20">
                    <ul>
                        <li>
                            <input type="checkbox" id="c1" name="cb">
                            <label for="c1">Price</label>
                        </li>
                        <li>
                            <input type="checkbox" id="c2" name="cb">
                            <label for="c2">Dish</label>
                        </li>
                        <li>
                            <input type="checkbox" id="c3" name="cb">
                            <label for="c3">Weight</label>
                        </li>
                        <li>
                            <input type="checkbox" id="c4" name="cb">
                            <label for="c4">Veg</label>
                        </li>
                        <li>
                            <input type="checkbox" id="c5" name="cb">
                            <label for="c5">Non-veg</label>
                        </li>
                        <li>
                            <input type="checkbox" id="c6" name="cb">
                            <label for="c6">All</label>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-xs-12 text-center">
                <a href="#" class="itg-button">Search</a>
            </div>
        </div>
    </div>
</div>
</section>
